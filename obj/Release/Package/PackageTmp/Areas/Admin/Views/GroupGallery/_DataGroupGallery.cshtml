@model IEnumerable<SinglePageApp.GroupGallery>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
           <a class="btn btn-warning" onclick="edit(@item.id)">ویرایش</a>
        </td>
    </tr>
}

</table>
